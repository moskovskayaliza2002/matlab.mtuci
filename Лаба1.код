a = 3.5;
b = -0.7;
i = 2;
x = 0.8;
disp('вычисление выражения:')
y = power(10,4) * power(sin(i),2) - (0.32*x*x*x + 4*x + b)/(cos(i*a) * power((0.32*x*x*x - b), 1/6)) + abs(power(10,-3)*tan(a*b))
mas = [a, b, i, x, y]
disp('отсортированный по возрастанию вектор:')
sort(mas)
disp('сложение отсортированного и исходного массива')
fin = mas + sort(mas)
b = find(fin>0);
disp('элементы массива, что привышают по значению 0')
c = fin(b)
